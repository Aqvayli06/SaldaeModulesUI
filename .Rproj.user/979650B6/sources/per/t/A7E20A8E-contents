library("tidyverse")
ui <- shinydashboard::dashboardPage(
  shinydashboard::dashboardHeader(title = "Shiny Modules"),
  shinydashboard::dashboardSidebar(disable = FALSE),
  shinydashboard::dashboardBody(

    tisefka_inu <- SaldaeModulesUI::ghred_tisefka_UI(id = "tisefka_tizegzawin"),

    SA_tisefka_UI("SA_module_tisefka",mod_title= "Saldae Module"),

    SaldaeModulesUI::SA_tisefka_multiple_UI("SA_multiple_test",mod_title= "Saldae Module"),

    SaldaeModulesUI::SA_Value_box_UI("SA_valuebox")

  )
)

server <- function(input, output, session) {

  tisefka_inu <- callModule(module = ghred_tisefka_mod, id = "tisefka_tizegzawin")
  tisefka_tizegzawin <- reactive({
    req(tisefka_inu())
    reactive({tisefka_inu()$tisefka_tizegzawin})
  })

  output$plot_inu <- callModule(module =  SA_tisefka_mod, id = "SA_module_tisefka",tisefka = reactive({tisefka_inu()}))

  callModule(module =  SA_tisefka_multiple_mod, id = "SA_multiple_test", tisefka = reactive({tisefka_inu()}))

  callModule(module = SaldaeModulesUI::SA_Value_box_server, id = "SA_valuebox", tisefka = tisefka_vb)

}

shinyApp(ui = ui, server = server)
